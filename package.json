{
  "name": "pandoc-svelte-components",
  "version": "0.0.6",
  "scripts": {
    "dev": "vite dev --port 2999",
    "build": "vite build && touch docs/.nojekyll # for GitHub Pages",
    "package": "svelte-package",
    "preview": "svelte-kit preview",
    "publish": "svelte-package && npm publish ./package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "exports": {
    "." : "./index.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.48",
    "@sveltejs/kit": "^1.0.0-next.561",
    "@sveltejs/package": "^1.0.0-next.6",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "carbon-icons-svelte": "^11.4.0",
    "d3-ease": "^3.0.1",
    "d3-selection": "^3.0.0",
    "d3-transition": "^3.0.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "highlight.js": "^11.7.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "node-pandoc-filter": "file:../node-pandoc-filter",
    "openseadragon": "^3.1.0",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "type": "module"
}
